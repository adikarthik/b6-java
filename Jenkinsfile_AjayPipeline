pipeline {

        agent any
        stages{

                        stage(clean){
                        steps{
                            dir('Ajay-MBPline'){
                                    sh 'mvn clean'
                            }
                        }
        }
                stage(install){
                        steps{
                            dir('Ajay-MBPline'){
                                    sh 'mvn install'
                        }
        }
                         }  
                 
                 }
             }
