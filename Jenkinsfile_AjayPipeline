pipeline {

        agent any
        stages{

                        stage(clean){
                        steps{
                            dir('adi-forked'){
                                    sh 'mvn clean'
                            }
                        }
        }
                stage(install){
                        steps{
                            dir('adi-forked'){
                                    sh 'mvn install'
                        }
        }
                            post{
                                always{
                                    archiveArtifacts artifacts: 'target/**.jar', followSymlinks: true
                                    junit 'target/surefire-reports/*.xml'
                                }
                            }
                       }
                                stage('clean ws'){
                                steps{
                                clean Ws()
                        }
                     }
                 }
             }
